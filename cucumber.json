{
  "default": {
      
      "formatOptions": {
          "snippetInterface": "async-await"
      },
      "publishQuiet": true,
      "dryRun": false,
      "paths": [
          "src/features/"
      ],
      "require": [
          "src/Step-definitions/*.ts"
      ],
      "requireModule": [
          "ts-node/register"
      ],
      "format": [
        "progress-bar",
        "html:test-results/reports/cucumber-report.html",
        "json:test-results/reports/cucumber-report.json",
        "rerun:@rerun.txt"
    ]
  }  
}